(val result 0)      ;;final result of choose(n k)
(val temp 0)
(define choose(n k)
    (if(= k n) 1 
        (if(= 0 k) 1
            (begin (set n (- n 1))
               (set temp k)
               (set k (- k 1))
               (set result (+ (choose n temp) (choose n k)))
            )
        )
    )
)

